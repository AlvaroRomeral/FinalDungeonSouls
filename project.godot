[application]

config/name="FinalDungeonSouls"
run/main_scene="res://Pantallas/Menus/MenuPrincipal.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="FinalDungeonSouls"
boot_splash/image="res://Recursos/Imagenes/Parallax/parallax-mountain-bg.png"
config/icon="res://FDS_icon.png"
config/windows_native_icon="res://FDS_icon.ico"

[autoload]

Global="*res://#Global.gd"
Datos="*res://#Datos.gd"
Jugador="*res://#Jugador.gd"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"

[display]

window/size/width=640
window/size/height=360
window/size/fullscreen=true
window/size/test_width=1920
window/size/test_height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-sqlite/plugin.cfg", "res://addons/silent_wolf/plugin.cfg" )

[gui]

theme/custom="res://Recursos/Estilos/Interfaz.tres"

